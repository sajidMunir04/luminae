{
  "compilerOptions": {
    "target": "es2017",
    "paths": {
      "@/*": ["./src/*"]
    },
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": false,
    "noEmit": true,
    "incremental": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "plugins": [
      {
        "name": "next"
      }
    ],
    "strictNullChecks": true
  },
  "include": [
    "next-env.d.ts",
    ".next/types/**/*.ts",
    "types/**/*.ts",
    "**/*.ts",
    "**/*.tsx",
    "pages/index.js"
, "pages/api/fetchProducts.ts", "pages/api/storeProduct.ts", "src/utilities/databasePopulator.mjs", "src/model/Product.mjs", "pages/[products]/[productCategory].js", "pages/ContextData.js", "pages/[products]/index.js", "pages/layout.js", "pages/_app.js", "databaseutilities/databasePopulator.mjs", "databaseutilities/producttypes.mjs", "src/app/api/auth/[...nextauth].js", "app/layout.tsx"  ],
  "exclude": [
    "node_modules"
  ]
}
